    <main>
        <div class="container">
            <a href="/profile">
                <span><i class="fa-solid fa-user"></i></span>&nbsp;
                <span>Account</span>
            </a>
            <a href="/settings">
                <span><i class="fa-solid fa-gear"></i></span>&nbsp;
                <span>Settings</span>
            </a>
        </div>
    </main>

    <script src="/js/toggle.js"></script>
